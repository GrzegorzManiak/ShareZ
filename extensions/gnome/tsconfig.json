{
    "compilerOptions": {
        "lib": [
            "ESNext"
        ],
        "types": [
            "node"
        ],
        "target": "ES2022",
        "module": "ESNext",
        "moduleResolution": "node",
        "rootDir": "./src",
        "outDir": "../../dist/gnome",
        "forceConsistentCasingInFileNames": true,
        "esModuleInterop": true,
        "removeComments": true,
        "strict": true,
        "skipLibCheck": true
    }
}
